        "## Comparing two different conditions"
      ],
      "metadata": {
        "id": "U5HfkM7maNDc"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def estimate_confidence_interval(data, column_heading=\"p03um\", percentile=0.975):\n",
        "    # prompt: write the code to calculate the average of a table column\n",
        "    mean = data.column(column_heading).mean()\n",
        "    std = data.column(column_heading).std()\n",
        "    n = len(data.column(column_heading))\n",
        "    standard_error =  std / np.sqrt(n)\n",
        "    # assuming 95% confidence level\n",
        "    margin_of_error = standard_error*inverse_cdf(percentile)\n",
        "    # estimating the average\n",
        "    print(f\"sample average: {int(mean)}\")\n",
        "    print(f\"sample standard deviation: {int(std)}\")\n",
        "    print(f\"standard error: {int(standard_error)}\")\n",
        "    print(f\"margin of error: {int(margin_of_error)}\")\n",
        "    print(f\"confidence interval: [{int(mean - margin_of_error)},{int(mean + margin_of_error)}] \")\n",
        "\n",
        "estimate_confidence_interval(data_table, \"p03um\", 0.975)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "8Ip8nvW0ajro",
        "outputId": "ce7d3fcd-4ef4-421a-fb9b-da39bd549f7b"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
