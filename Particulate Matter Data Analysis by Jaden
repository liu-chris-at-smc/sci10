        "standard way for making a histogram from a data table object"
      ],
      "metadata": {
        "id": "aKdUBQllHsUg"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# approximate bin size\n",
        "min_count = light_data_table.column(\"p03um\").min()\n",
        "max_count = light_data_table.column(\"p03um\").max()\n",
        "range = max_count - min_count\n",
        "num_of_measurements = len(light_data_table.column(\"p03um\"))\n",
        "# marder recommendation for number of bins\n",
        "num_bins = np.sqrt(num_of_measurements)*2\n",
        "# round to tens place\n",
        "approx_bin_width = ((range / num_bins ) / 10)\n",
        "print(f\"min_count: {min_count}\")\n",
        "print(f\"max_count: {max_count}\")\n",
        "print(f\"approximate bin width: {approx_bin_width}\")"
      ],
